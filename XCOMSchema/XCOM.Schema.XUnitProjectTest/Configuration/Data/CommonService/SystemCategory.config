    <!--获取子品类-->
    <SQL SQLKey="GetSystemCategoryChildren" ConnectionKey="AuthCenterWrite">
      <Text>
        <![CDATA[
SELECT  SysNo
FROM    YZ_Operation.dbo.SystemCategory WITH ( NOLOCK )
WHERE   ParentSysNo = @SysNo;
          ]]>
      </Text>
    </SQL>

