    <!-- 通过日志ID获取系统日志详细 -->
    <SQL SQLKey="CommonService_GetSytemEventLogContent" ConnectionKey="MainRead">
      <Text>
        <![CDATA[
        SELECT * FROM [YZ_Log_2017].dbo.[SystemEventLog] WITH(NOLOCK) WHERE LogID = @LogID
          ]]>
      </Text>
    </SQL>
    

    
