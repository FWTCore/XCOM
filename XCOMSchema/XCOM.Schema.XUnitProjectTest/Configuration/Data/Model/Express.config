		<SQL SQLKey="express.template_model_Param" ConnectionKey="localhost">
			<Text>
				<![CDATA[
  
SELECT  SysNo
FROM    express
WHERE   SysNo = @SysNo
and CompanyName=@CompanyName
and CommonStatus=@CommonStatus
;

          ]]>
			</Text>
		</SQL>


		<SQL SQLKey="express.template_condition" ConnectionKey="MZXSystem">
